<h3>Income</h3>

<div class="row lead">
	<div class="col-md-3">
		<%= render 'expenses/year_navigator' %>
	</div>
</div>

<div class="row lead">
	<div class="col-md-3">
		<%= render 'expenses/month_navigator' %>
	</div>
</div>

<div class="container">
  <div class="row">
    <div class="col-md-10">
        <%== pagy_bootstrap_nav(@pagy) %>
    </div>
    <div class="col-md-2">
      <%= link_to "View Uncategorized", uncategorized_incomes_path %>
    </div>
  </div>
</div>

<%= render partial: "transactions/typed_transaction_list", locals: {txType: 'income'} %>

<div class="row">
	<div class="col-md-12 text-right">
		<button id="exportCsv" type="submit" class="btn mb-1" onclick="exportCsv()">Download as CSV</button>
	</div>
</div>