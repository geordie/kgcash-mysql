<h1>Listing budgets</h1>

<table>
  <tr>
    <th>Category</th>
    <th>Amount</th>
    <th>Period</th>
  </tr>

<% @budget_categories.each do |budget_category| %>
  <tr>
    <td><%= budget_category.category_id %></td>
    <td><%= budget_category.amount %></td> 
    <td><%= budget_category.period %></td>

  </tr>
<% end %>
</table>

