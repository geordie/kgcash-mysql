<h1>New Document</h1>

<%= form_with model: @document do |form| %>
	<div class="form-group">
		<%= form.label :name %><br>
		<%= form.text_field :name %>
	</div>

	<div class="form-group">
		<%= form.label :file %>
		<%= form.file_field :file %>	
	</div>

	<div>
		<%= form.submit %>
	</div>
<% end %>